<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.neuedu.dao.SwiperDao">

	<select id="findAllByEle" resultType="String">
		select imgurl
		from swiper
		where qid = #{qid}
		and category = #{category}
	</select>

</mapper>