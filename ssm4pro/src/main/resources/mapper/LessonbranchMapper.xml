<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.neuedu.dao.LessonbranchDao">
	<insert id="insert" useGeneratedKeys="true">
		insert into lessonbranch (lid, branchid)
		values (#{lid}, #{branchid});
	</insert>
	
	<update id="update">
		update lessonbranch set
			lid = #{lid},
			branchid = #{branchid}
		where id = #{id}
	</update>
	
	<delete id="deleteLesson">
		delete from lessonbranch
		where id = #{id}
	</delete>
	
	<select id="findById" resultType="Lessonbranch">
		select *
		from lessonbranch
		where id = #{id}
	</select>
	
	<select id="findByLid" resultType="Lessonbranch">
		select *
		from lessonbranch
		where lid = #{lid}
	</select>
</mapper>
