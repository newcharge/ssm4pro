	<!-- 设置映射文件所在的位置 -->
	<mappers>
		<mapper resource="mapper/AddressMapper.xml"/>
		<mapper resource="mapper/CouponMapper.xml"/>
		<mapper resource="mapper/EnterpriseMapper.xml"/>
		<mapper resource="mapper/FreelistenbookMapper.xml"/>
		<mapper resource="mapper/FreelistenMapper.xml"/>
		<mapper resource="mapper/LessonbranchMapper.xml"/>
		<mapper resource="mapper/LessonMapper.xml"/>
		<mapper resource="mapper/MessageimgMapper.xml"/>
		<mapper resource="mapper/MessagelikeMapper.xml"/>
		<mapper resource="mapper/MessageMapper.xml"/>
		<mapper resource="mapper/MessagereplyMapper.xml"/>
		<mapper resource="mapper/PointMapper.xml"/>
		<mapper resource="mapper/RefundMapper.xml"/>
		<mapper resource="mapper/SignMapper.xml"/>
		<mapper resource="mapper/SorderMapper.xml"/>
		<mapper resource="mapper/SwiperMapper.xml"/>
		<mapper resource="mapper/TeacherMapper.xml"/>
		<mapper resource="mapper/UserMapper.xml"/>
	</mappers>
</configuration>
